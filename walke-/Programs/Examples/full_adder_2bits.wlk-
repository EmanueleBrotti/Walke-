- FULL ADDER, 2 BITS, IN WALKE

- inputs
SWITCH = #FFFF02, #FFFF01, #FFFF04, #FFFF03 -a,b,c,d

- outputs
OUTPUT = #00FF01,#00FF02,#00FF03  -SUM

- half adder + full adder
#FFFF01 XOR #FFFF03 = #00FF03
#FFFF01 AND #FFFF03 = #000000
#FFFF02 XOR #FFFF04 = #000001
#FFFF02 AND #FFFF04 = #000002
#000001 XOR #000000 = #00FF02
#000001 AND #000000 = #000003
#000003 OR #000002 = #00FF01
